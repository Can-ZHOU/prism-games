module bla
	
	s : [0..5] init 0;
	
	[a] s=0 -> (s'=1);
	[b] s=0 -> 1/2 : (s'=1) + 1/2 : (s'=2);

	[c] s=2 -> (s'=4);
	[d] s=2 -> (s'=3);

	[e] s=1 -> (s'=3);
	[f] s=1 -> 1/3 : (s'=0) + 1/3 : (s'=2) + 1/3 : (s'=4);

	[g] s>2 -> true;

endmodule